From 11b0d6532f19cc3da9175afa374124afdde2d970 Mon Sep 17 00:00:00 2001
From: Guillaume Tucker <guillaume.tucker@collabora.com>
Date: Thu, 14 Feb 2019 15:16:26 +0000
Subject: [PATCH 1/5] STAGING build-configs.yaml: add kernelci_staging-* build
 configs

---
 build-configs.yaml | 39 +++++++++++++++++++++++++++++++++++++++
 1 file changed, 39 insertions(+)

diff --git a/build-configs.yaml b/build-configs.yaml
index ef51e884c3d9..5d99ddb06d0b 100644
--- a/build-configs.yaml
+++ b/build-configs.yaml
@@ -756,6 +756,45 @@ build_configs:
     branch: 'kernelci.org'
     variants: *stable_variants
 
+  kernelci_staging-mainline:
+    tree: kernelci
+    branch: 'staging-mainline'
+    variants:
+      gcc-8: &staging_gcc
+        build_environment: gcc-8
+        fragments: [kselftest]
+        architectures:
+          arc: *arc_defconfig
+          arm: *arm_defconfig
+          arm64: *arm64_defconfig
+          i386: *i386_defconfig
+          mips: *mips_defconfig
+          riscv: *riscv_defconfig
+          x86_64: *x86_64_defconfig
+      clang-10: &staging_clang
+        build_environment: clang-10
+        fragments: [kselftest]
+        architectures:
+          arm: *arm_defconfig
+          arm64: *arm64_defconfig
+          x86_64: *x86_64_defconfig
+
+  kernelci_staging-next:
+    tree: kernelci
+    branch: 'staging-next'
+    variants:
+      gcc-8: *staging_gcc
+      clang-11:
+        <<: *staging_clang
+        build_environment: clang-11
+
+  kernelci_staging-stable:
+    tree: kernelci
+    branch: 'staging-stable'
+    variants:
+      gcc-8: *staging_gcc
+      clang-10: *staging_clang
+
   khilman:
     tree: khilman
     branch: 'to-build'
-- 
