From b2cfc04876966cec0b24bfa9fce53862b761b26c Mon Sep 17 00:00:00 2001
From: Guillaume Tucker <guillaume.tucker@collabora.com>
Date: Thu, 14 Feb 2019 15:16:26 +0000
Subject: [PATCH 3/8] STAGING add staging test build configs

---
 build-configs.yaml | 38 ++++++++++++++++++++++++++++++++++++++
 1 file changed, 38 insertions(+)

diff --git a/build-configs.yaml b/build-configs.yaml
index c7346191d033..b2b6942dbe79 100644
--- a/build-configs.yaml
+++ b/build-configs.yaml
@@ -103,6 +103,9 @@ trees:
   stable-rc:
     url: "https://git.kernel.org/pub/scm/linux/kernel/git/stable/linux-stable-rc.git"
 
+  staging:
+    url: "https://gitlab.collabora.com/gtucker/linux.git"
+
   tegra:
     url: "https://git.kernel.org/pub/scm/linux/kernel/git/tegra/linux.git"
 
@@ -429,6 +432,41 @@ build_configs:
     tree: evalenti
     branch: 'for-kernelci'
 
+  staging:
+    tree: staging
+    branch: 'kernelci-stable'
+    variants:
+      gcc-7:
+        build_environment: gcc-7
+        architectures:
+          arc:
+            base_defconfig: 'nsim_hs_defconfig'
+            filters:
+              - whitelist: {defconfig: ['nsim_hs_defconfig']}
+          arm:
+            base_defconfig: 'multi_v7_defconfig'
+            filters:
+              - whitelist: {defconfig: ['multi_v7_defconfig']}
+            fragments: [virtualvideo]
+          arm64: &staging_arm64
+            filters:
+              - whitelist: {defconfig: ['defconfig']}
+          mips:
+            base_defconfig: '32r2el_defconfig'
+            filters:
+              - whitelist: {defconfig: ['32r2el_defconfig']}
+          riscv:
+            filters:
+              - whitelist: {defconfig: ['defconfig']}
+          x86_64:
+            base_defconfig: 'x86_64_defconfig'
+            filters:
+              - whitelist: {defconfig: ['x86_64_defconfig']}
+      clang-8:
+        build_environment: clang-8
+        architectures:
+          arm64: *staging_arm64
+
   gtucker_stable:
     tree: gtucker
     branch: 'kernelci-stable'
-- 
