From 1edfaec3969a17c796a5e0e58c2bf2881e28bad1 Mon Sep 17 00:00:00 2001
From: Guillaume Tucker <guillaume.tucker@collabora.com>
Date: Thu, 14 Feb 2019 15:16:26 +0000
Subject: [PATCH 3/4] STAGING build-configs.yaml: add kernelci_staging build
 config

---
 build-configs.yaml | 36 ++++++++++++++++++++++++++++++++++++
 1 file changed, 36 insertions(+)

diff --git a/build-configs.yaml b/build-configs.yaml
index d3e8b790d384..8be876f9251b 100644
--- a/build-configs.yaml
+++ b/build-configs.yaml
@@ -504,6 +504,42 @@ build_configs:
     branch: 'kernelci.org'
     variants: *stable_variants
 
+  kernelci_staging:
+    tree: kernelci
+    branch: 'staging.kernelci.org'
+    variants:
+      gcc-8:
+        build_environment: gcc-8
+        architectures:
+          arc:
+            base_defconfig: 'nsim_hs_defconfig'
+            filters:
+              - whitelist: {defconfig: ['nsim_hs_defconfig']}
+          arm: &staging_arm
+            base_defconfig: 'multi_v7_defconfig'
+            filters:
+              - whitelist: {defconfig: ['multi_v7_defconfig']}
+          arm64: &staging_arm64
+            filters:
+              - whitelist: {defconfig: ['defconfig']}
+            fragments: [kselftest]
+          mips:
+            base_defconfig: '32r2el_defconfig'
+            filters:
+              - whitelist: {defconfig: ['32r2el_defconfig']}
+          riscv:
+            filters:
+              - whitelist: {defconfig: ['defconfig']}
+          x86_64:
+            base_defconfig: 'x86_64_defconfig'
+            filters:
+              - whitelist: {defconfig: ['x86_64_defconfig']}
+      clang-8:
+        build_environment: clang-8
+        architectures:
+          arm: *staging_arm
+          arm64: *staging_arm64
+
   khilman:
     tree: khilman
     branch: 'to-build'
-- 
