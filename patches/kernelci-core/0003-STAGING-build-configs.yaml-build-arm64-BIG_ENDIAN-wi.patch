Subject: [PATCH 3/8] STAGING build-configs.yaml: build arm64 BIG_ENDIAN with
 gcc and clang

---
 build-configs.yaml | 8 ++++++--
 1 file changed, 6 insertions(+), 2 deletions(-)

diff --git a/build-configs.yaml b/build-configs.yaml
index 44561210e22a..a7bff686c7da 100644
--- a/build-configs.yaml
+++ b/build-configs.yaml
@@ -618,9 +618,11 @@ build_configs:
             base_defconfig: 'multi_v7_defconfig'
             filters:
               - whitelist: {defconfig: ['multi_v7_defconfig']}
-          arm64: &staging_arm64
+          arm64:
             filters:
               - whitelist: {defconfig: ['defconfig']}
+            extra_configs:
+              - 'defconfig+CONFIG_CPU_BIG_ENDIAN=y'
           i386:
             base_defconfig: 'i386_defconfig'
             filters:
@@ -640,7 +642,9 @@ build_configs:
         build_environment: clang-9
         architectures:
           arm: *staging_arm
-          arm64: *staging_arm64
+          arm64:
+            extra_configs:
+              - 'defconfig+CONFIG_CPU_BIG_ENDIAN=y'
           x86_64: *staging_x86_64
 
