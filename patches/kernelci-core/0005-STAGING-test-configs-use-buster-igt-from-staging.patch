 test-configs.yaml | 10 ++++++++--
 1 file changed, 8 insertions(+), 2 deletions(-)

diff --git a/test-configs.yaml b/test-configs.yaml
index 1163314ad9f4..bc817fd98202 100644
--- a/test-configs.yaml
+++ b/test-configs.yaml
@@ -20,6 +20,12 @@ file_system_types:
       armhf: [{arch: arm}]
       amd64: [{arch: x86_64}]
 
+  debian_staging:
+    url: 'http://staging-storage.kernelci.org/images/rootfs/debian'
+    arch_map:
+      armhf: [{arch: arm}]
+      amd64: [{arch: x86_64}]
+
 
 file_systems:
 
@@ -44,8 +50,8 @@ file_systems:
     ramdisk: 'buster/20190705.0/{arch}/rootfs.cpio.gz'
 
   debian_buster-igt_ramdisk:
-    type: debian
-    ramdisk: 'buster-igt/20190705.0/{arch}/rootfs.cpio.gz'
+    type: debian_staging
+    ramdisk: 'buster-igt/20190704.0/{arch}/rootfs.cpio.gz'
 
   debian_stretch-v4l2_ramdisk:
     type: debian
