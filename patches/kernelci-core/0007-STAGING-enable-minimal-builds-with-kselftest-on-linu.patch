Subject: [PATCH 7/7] STAGING enable minimal builds with kselftest on
 linux-5.8.y

---
 build-configs.yaml | 19 +++++++++++++++++++
 1 file changed, 19 insertions(+)

diff --git a/build-configs.yaml b/build-configs.yaml
index 97d0dd1b04a9..7c13d810827d 100644
--- a/build-configs.yaml
+++ b/build-configs.yaml
@@ -724,6 +724,25 @@ build_configs:
           arm64: *staging_arm64
           x86_64: *staging_x86_64
 
+  kernelci_staging-stable:
+    tree: kernelci
+    branch: 'staging-stable'
+    variants:
+      gcc-8:
+        build_environment: gcc-8
+        fragments: [kselftest]
+        architectures:
+          arm: *arm_defconfig
+          arm64: *arm64_defconfig
+          x86_64: *x86_64_defconfig
+      clang-11:
+        build_environment: clang-11
+        fragments: [kselftest]
+        architectures:
+          arm: *arm_defconfig
+          arm64: *arm64_defconfig
+          x86_64: *x86_64_defconfig
+
   khilman:
     tree: khilman
     branch: 'to-build'
