From 9d1813c16389edb11761d781cb5802b75a30b281 Mon Sep 17 00:00:00 2001
From: Guillaume Tucker <guillaume.tucker@collabora.com>
Date: Fri, 31 May 2019 17:35:13 +0100
Subject: [PATCH 8/8] STAGING test-configs.yaml: use buster and buster-igt from
 staging

---
 test-configs.yaml | 30 ++++++++++++++++++++----------
 1 file changed, 20 insertions(+), 10 deletions(-)

diff --git a/test-configs.yaml b/test-configs.yaml
index 9fa213c0fd58..9c21792edfd8 100644
--- a/test-configs.yaml
+++ b/test-configs.yaml
@@ -20,6 +20,12 @@ file_system_types:
       armhf: [{arch: arm}]
       amd64: [{arch: x86_64}]
 
+  debian_staging:
+    url: 'http://staging-storage.kernelci.org/images/rootfs/debian'
+    arch_map:
+      armhf: [{arch: arm}]
+      amd64: [{arch: x86_64}]
+
 
 file_systems:
 
@@ -39,14 +45,18 @@ file_systems:
     type: buildroot
     ramdisk: '{arch}/tests/rootfs.cpio.gz'
 
+  debian_buster_ramdisk:
+    type: debian_staging
+    ramdisk: 'buster/20190531.0/{arch}/rootfs.cpio.gz'
+
+  debian_buster-igt_ramdisk:
+    type: debian_staging
+    ramdisk: 'buster-igt/20190603.2/{arch}/rootfs.cpio.gz'
+
   debian_stretch_ramdisk:
     type: debian
     ramdisk: 'stretch/20190531.0/{arch}/rootfs.cpio.gz'
 
-  debian_stretch-igt_ramdisk:
-    type: debian
-    ramdisk: 'stretch-igt/20190531.0/{arch}/rootfs.cpio.gz'
-
   debian_stretch-v4l2_ramdisk:
     type: debian
     ramdisk: 'stretch-v4l2/20190531.0/{arch}/rootfs.cpio.gz'
@@ -104,10 +114,10 @@ test_plans:
     pattern: 'boot/generic-qemu-boot-template.jinja2'
 
   cros-ec:
-    rootfs: debian_stretch_ramdisk
+    rootfs: debian_buster_ramdisk
 
   igt:
-    rootfs: debian_stretch-igt_ramdisk
+    rootfs: debian_buster-igt_ramdisk
 
   kselftest:
     rootfs: buildroot_kselftest_ramdisk
@@ -115,18 +125,18 @@ test_plans:
       - whitelist: {defconfig: ['kselftest']}
 
   simple:
-    rootfs: debian_stretch_ramdisk
+    rootfs: debian_buster_ramdisk
 
   simple_qemu:
     name: simple
-    rootfs: debian_stretch_ramdisk
+    rootfs: debian_buster_ramdisk
     pattern: 'simple/generic-qemu-simple-template.jinja2'
 
   sleep:
-    rootfs: debian_stretch_ramdisk
+    rootfs: debian_buster_ramdisk
 
   usb:
-    rootfs: debian_stretch_ramdisk
+    rootfs: debian_buster_ramdisk
 
   v4l2_compliance_uvc:
     name: v4l2-compliance-uvc
-- 
2.20.1

