From 5977552dedd36d3c360570caa6b633d11a8ca51d Mon Sep 17 00:00:00 2001
From: Guillaume Tucker <guillaume.tucker@collabora.com>
Date: Thu, 14 Feb 2019 15:16:26 +0000
Subject: [PATCH 2/4] CHROMEOS build-configs.yaml: add kernelci_chromeos-*
 build configs

---
 config/core/build-configs.yaml | 47 ++++++++++++++++++++++++++++++++++
 1 file changed, 47 insertions(+)

diff --git a/config/core/build-configs.yaml b/config/core/build-configs.yaml
index d04e07c2bd12..a19693cf8aca 100644
--- a/config/core/build-configs.yaml
+++ b/config/core/build-configs.yaml
@@ -662,6 +662,53 @@ build_configs:
     tree: evalenti
     branch: 'for-kernelci'
 
+  kernelci_chromeos-mainline:
+    tree: kernelci
+    branch: 'chromeos-mainline'
+    variants:
+      gcc-8: &chromeos_gcc
+        build_environment: gcc-8
+        fragments: [kselftest]
+        architectures:
+          arc: *arc_defconfig
+          arm: *arm_defconfig
+          arm64: *arm64_defconfig
+          i386: *i386_defconfig
+          mips: *mips_defconfig
+          riscv: *riscv_defconfig
+          x86_64:
+            <<: *x86_64_defconfig
+            fragments: [x86-chromebook]
+            extra_configs: ['x86_64_defconfig+x86-chromebook+kselftest']
+      clang-10: &chromeos_clang
+        build_environment: clang-10
+        fragments: [kselftest]
+        architectures:
+          arm: *arm_defconfig
+          arm64: *arm64_defconfig
+          x86_64:
+            <<: *x86_64_defconfig
+            fragments: [x86-chromebook]
+            extra_configs: ['x86_64_defconfig+x86-chromebook+kselftest']
+      chromeos-configs:
+        <<: *chromeos-configs
+
+  kernelci_chromeos-next:
+    tree: kernelci
+    branch: 'chromeos-next'
+    variants:
+      gcc-8: *chromeos_gcc
+      clang-13:
+        <<: *chromeos_clang
+        build_environment: clang-13
+
+  kernelci_chromeos-stable:
+    tree: kernelci
+    branch: 'chromeos-stable'
+    variants:
+      gcc-8: *chromeos_gcc
+      clang-10: *chromeos_clang
+
   khilman:
     tree: khilman
     branch: 'to-build'
-- 
2.20.1

