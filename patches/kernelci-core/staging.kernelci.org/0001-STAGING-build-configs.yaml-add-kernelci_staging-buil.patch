From 68935eb5c41ef386a6e1e24e510ec69ef26e9649 Mon Sep 17 00:00:00 2001
From: Guillaume Tucker <guillaume.tucker@collabora.com>
Date: Thu, 14 Feb 2019 15:16:26 +0000
Subject: [PATCH 01/10] STAGING build-configs.yaml: add kernelci_staging-*
 build configs

---
 config/core/build-configs.yaml | 46 ++++++++++++++++++++++++++++++++++
 1 file changed, 46 insertions(+)

diff --git a/config/core/build-configs.yaml b/config/core/build-configs.yaml
index f3e3bd43198a..f9bb9d2abd51 100644
--- a/config/core/build-configs.yaml
+++ b/config/core/build-configs.yaml
@@ -625,6 +625,52 @@ build_configs:
     tree: evalenti
     branch: 'for-kernelci'
 
+  kernelci_staging-mainline:
+    tree: kernelci
+    branch: 'staging-mainline'
+    variants:
+      gcc-10: &staging_gcc
+        build_environment: gcc-10
+        fragments: [kselftest]
+        architectures:
+          arc: *arc_defconfig
+          arm: *arm_defconfig
+          arm64: *arm64_defconfig
+          i386: *i386_defconfig
+          mips: *mips_defconfig
+          riscv: *riscv_defconfig
+          x86_64:
+            <<: *x86_64_defconfig
+            fragments: [x86-chromebook]
+            extra_configs: ['x86_64_defconfig+x86-chromebook+kselftest']
+      clang-11: &staging_clang
+        build_environment: clang-11
+        fragments: [kselftest]
+        architectures:
+          arm: *arm_defconfig
+          arm64: *arm64_defconfig
+          x86_64:
+            <<: *x86_64_defconfig
+            fragments: [x86-chromebook]
+            extra_configs: ['x86_64_defconfig+x86-chromebook+kselftest']
+
+
+  kernelci_staging-next:
+    tree: kernelci
+    branch: 'staging-next'
+    variants:
+      gcc-10: *staging_gcc
+      clang-13:
+        <<: *staging_clang
+        build_environment: clang-13
+
+  kernelci_staging-stable:
+    tree: kernelci
+    branch: 'staging-stable'
+    variants:
+      gcc-10: *staging_gcc
+      clang-11: *staging_clang
+
   khilman:
     tree: khilman
     branch: 'to-build'
-- 
2.20.1

